<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{ site.language_attributes }}> <!--<![endif]-->
<head>
    <meta charset="{{ site.charset }}" />
        <title>   
            {% if wp_title %}
                {{ wp_title }} - {{ site.name }}
            {% else %}
                {{ site.name }}
            {% endif %}
        </title>
    <meta name="description" content="{{ site.description }}">
    <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="author" href="{{ site.theme.link }}/humans.txt" />
    <link rel="pingback" href="{{ site.pingback_url }}" />


    <!-- Template CSS Files
        ================================================== -->
    <!-- Twitter Bootstrs CSS -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/bootstrap.min.css">
    <!-- Ionicons Fonts Css -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/ionicons.min.css">
    <!-- animate css -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/animate.css">
    <!-- Hero area slider css-->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/slider.css">
    <!-- owl craousel css -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/owl.carousel.css">
    <link rel="stylesheet" href="{{ site.theme.link }}/css/owl.theme.css">
    <link rel="stylesheet" href="{{ site.theme.link }}/css/jquery.fancybox.css">
    <!-- template main css file -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/main.css">
    <!-- responsive css -->
    <link rel="stylesheet" href="{{ site.theme.link }}/css/responsive.css">

    <!-- Template Javascript Files
    ================================================== -->
    <!-- modernizr js -->
    <script src="{{ site.theme.link }}/js/vendor/modernizr-2.6.2.min.js"></script>
    <!-- jquery -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <!-- owl carouserl js -->
    <script src="{{ site.theme.link }}/js/owl.carousel.min.js"></script>
    <!-- bootstrap js -->

    <script src="{{ site.theme.link }}/js/bootstrap.min.js"></script>
    <!-- wow js -->
    <script src="{{ site.theme.link }}/js/wow.min.js"></script>
    <!-- slider js -->
    <script src="{{ site.theme.link }}/js/slider.js"></script>
    <script src="{{ site.theme.link }}/js/jquery.fancybox.js"></script>
    <!-- template main js -->
    <script src="{{ site.theme.link }}/js/main.js"></script>

    {{function('wp_head')}}
